export interface ModuleInfo {
  id: string;
  name: string;
  title: string;
  icon: string;
  iconAlt: string;
  backgroundColor: string;
  backUrl: string;
  progressLabel?: string;
}

export interface GameInfo {
  title: string;
  description: string;
  tags: string[];
}

export interface GameStats {
  aciertos: number;
  errores: number;
  completedSets: number;
  totalSets: number;
  level?: number;
  totalAciertos?: number;
}